{
  "name": "upload-artifact",
  "version": "2.0.1",
  "description": "Upload a build artifact that can be used by subsequent workflow steps",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "release": "ncc build src/upload-artifact.ts && git add -f dist/",
    "check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:test\" \"npm:build\"",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "test": "jest --testTimeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/upload-artifact.git"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Artifacts",
    "Upload"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actions/upload-artifact/issues"
  },
  "homepage": "https://github.com/actions/upload-artifact#readme",
  "dependencies": {
    "@actions/artifact": "^0.5.2",
    "@actions/core": "^1.9.0",
    "@actions/glob": "^0.1.2",
    "@actions/io": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.52",
    "@typescript-eslint/parser": "^2.34.0",
    "@zeit/ncc": "^0.22.3",
    "concurrently": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^23.20.0",
    "glob": "^7.2.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "^2.7.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.10"
  }
}
